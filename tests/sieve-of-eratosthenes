func SoE(limit: real_int) {
  marks = [false] * (limit + 1);
  
  for (i = 2; i*i <= limit; i = i + 1) {
    if (!marks.i) {
      for (j = i*i; j <= limit; j = j + i) {
        set(marks, j, true);
      }
    }
  }

  primes = [];
  for (i = 0; i < len(marks); i = i + 1) {
    if (!marks.i) {
      push(primes, i);
    }
  }
  primes;
}

limit = argv.0;
primes = SoE(limit);
println("There are " + len(primes) + " primes <= " + limit + ":");
println(primes);